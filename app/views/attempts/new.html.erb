<%= form_for @new_attempt, :url => '/attempt/create', :html => { :multipart => true } do |f| %>
	Show us a picture of you doing this! <br>
	<%= f.file_field :avatar, :required => true %> <br>

	Write a little something about this! <br>
	<%= f.text_field :caption %> <br>


	<%= f.submit "POST!" %>
<% end %>